{
  "suggest.enablePreselect": true,
  //"suggest.selection": "first",
  "suggest.defaultSortMethod": "length",
  "suggest.snippetsSupport": true,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.noselect": false,
  "suggest.virtualText": true,
  //"diagnostic.virtualText": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualTextCurrentLineOnly": true,
  "diagnostic.virtualTextPrefix": "",
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",
  "list.indicator": ">",
  "list.selectedSignText": " ",
  "tailwindCSS.trace.server": "messages",
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.enable": true,
  "tailwindCSS.validate": true,
  "tailwindCSS.lint.cssConflict": "warning",
  "suggest.removeDuplicateItems": true,
  "tailwindCSS.headwind.removeDuplicates": true,
  "tailwindCSS.headwind.runOnSave": false,
  "lua.enable": true,
  "coc.source.emoji.enable": true,
  "coc.source.emoji.filetypes": ["markdown"],
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "graphql",
    "html",
    "yaml",
    "json",
    "python"
  ],
  "pairs.enableBackspace": true,
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
  "typescript.autoClosingTags": true,
  "typescript.suggest.enabled": true,
  "typescript.suggest.autoImports": true,
  "typescript.suggestionActions.enabled": true,
  "javascript.autoClosingTags": true,
  "snippets.autoTrigger": true,
  "snippets.shortcut": "S",
  "snippets.ultisnips.directories": ["UltiSnips", "~/.config/nvim/snips"],
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "yank.priority": 10,
  "tabnine.priority": 1,
  "snippets.priority": 999,
  "emmet.priority": 200,
  "floaterm.completion.priority": 5,
  "suggest.languageSourcePriority": 10,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.showExpandedAbbreviation": "always",
  "emmet.showAbbreviationSuggestions": true,
  "explorer.width": 35,
  "explorer.position": "right",
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.enableDevicons": true,
  "explorer.icon.expanded": "",
  "explorer.icon.collapsed": "",
  "explorer.icon.link": "→",
  "tsserver.enable": true,
  "eslint.enable": true,
  "eslint.run": "onSave",
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "extensinons": [".ts", ".js", ".tsx", ".jsx"]
  },
  "explorer.keyMappings.global": {
    "mc": "copyFile",
    "C": "copyFile",
    "md": "delete",
    "D": "delete",
    "II": "preview:content"
  },
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  }
}
