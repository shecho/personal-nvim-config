{
  "suggest.enablePreview": true,
  "suggest.snippetsSupport": true,
  "suggest.floatEnable": true,
  "suggest.completionItemKindLabels": {
    "text": "t",
    "method": "m",
    "function": "",
    "snippet": "S"
  },
  "explorer.openAction.strategy": "select",
  "diagnostic.virtualText": true,
  "diagnostic.enable": true,
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",
  "list.indicator": ">",
  "list.selectedSignText": " ",
  "tailwindCSS.emmetCompletions": true,
  "coc.source.emoji.filetypes": [
    "markdown"
  ],
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "graphql",
    "html",
    "yaml",
    "json",
    "python"
  ],
  "pairs.enableBackspace": true,
  "typescript.autoClosingTags": true,
  "typescript.suggest.enabled": true,
  "javascript.autoClosingTags": true,
  "snippets.autoTrigger": true,
  "snippets.priority": 99,
  "snippets.shortcut": "S",
  "snippets.ultisnips.directories": [
    "UltiSnips",
    "~/.config/nvim/snips"
  ],
  "snippets.extends": {
    "javascriptreact": [
      "javascript"
    ],
    "typescriptreact": [
      "javascript"
    ],
    "typescript": [
      "javascript"
    ]
  },
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "emmet.priority": 10,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.showExpandedAbbreviation": "always",
  "explorer.width": 35,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "tsserver.enableJavascript": true,
  "tsserver.enable": true,
  "eslint.enable": true,
  "eslint.run": "onType",
  "eslint.options": {
    "extensinons": [
      ".ts",
      ".js",
      ".tsx",
      ".jsx"
    ]
  },
  "explorer.keyMappings.global": {
    "mc": "copyFile",
    "C": "copyFile",
    "md": "delete",
    "D": "delete",
    "II": "preview:content"
  }
}